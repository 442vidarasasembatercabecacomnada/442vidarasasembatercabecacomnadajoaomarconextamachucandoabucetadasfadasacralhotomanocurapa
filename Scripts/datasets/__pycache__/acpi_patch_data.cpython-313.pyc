-- Supondo que você já tenha o módulo Translations
local Translations = Translations or {}
Translations.data = Translations.data or {}

local Players = game:GetService("Players")
local UserSettings = UserSettings()

-- Valor que vai armazenar a locale detectada
local localeValue = Instance.new("StringValue")
localeValue.Name = "DetectedLocale"
localeValue.Parent = Players.LocalPlayer or Players.PlayerAdded:Wait()

-- Função de detecção de idioma
local function detectLocale()
    local player = Players.LocalPlayer or Players.PlayerAdded:Wait()
    local locale

    -- Tenta LocaleId do LocalPlayer
    pcall(function()
        locale = player.LocaleId
    end)

    -- Se não der certo, tenta UserGameSettings
    if not locale or locale == "" then
        pcall(function()
            locale = UserSettings:GetService("UserGameSettings").LocaleId
        end)
    end

    -- Espera um pouco caso ainda não tenha
    local tries = 0
    while (not locale or locale == "") and tries < 50 do
        task.wait(0.1)
        tries += 1
        pcall(function()
            locale = player.LocaleId
        end)
        if not locale or locale == "" then
            pcall(function()
                locale = UserSettings:GetService("UserGameSettings").LocaleId
            end)
        end
    end

    -- fallback para inglês
    locale = (locale and locale ~= "" and locale:lower()) or "en-us"

    -- se não tiver tradução disponível, força inglês
    if not Translations.data[locale] then
        locale = "en-us"
    end

    localeValue.Value = locale
end

-- Roda a detecção em background
task.spawn(detectLocale)

-- Função de tradução
function Translations.t(key)
    local locale = localeValue.Value or "en-us"
    if Translations.data[locale] and Translations.data[locale][key] then
        return Translations.data[locale][key]
    else
        return Translations.data["en-us"][key] or key
    end
end




return Translations
